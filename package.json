{
  "engines": {
    "node": ">=14.0.0",
    "yarn": ">=1.22.0",
    "npm": "please-use-yarn"
  },
  "scripts": {
    "start": "node dist/index.js",
    "server:dev": "nodemon dist/index.js",
    "start-ts": "ts-node src/index.ts",
    "dev-ts": "nodemon --exec ts-node src/index.ts",
    "watch:dev": "tsc -w",
    "check": "tsc --traceResolution",
    "prepare": "husky install"
  },
  "dependencies": {
    "argon2": "^0.28.7",
    "cors": "^2.8.5",
    "crypto": "^1.0.1",
    "dayjs": "^1.11.4",
    "dotenv": "^16.0.1",
    "express": "^4.18.1",
    "joi": "^17.6.0",
    "jsonwebtoken": "^8.5.1",
    "lodash": "^4.17.21",
    "module-alias": "^2.2.2",
    "mongodb": "^4.8.0",
    "nanoid": "^4.0.0",
    "pino": "^8.2.0",
    "pino-pretty": "^8.1.0"
  },
  "devDependencies": {
    "@commitlint/cli": "^17.0.3",
    "@commitlint/config-conventional": "^17.0.3",
    "@types/argon2": "^0.15.0",
    "@types/body-parser": "^1.19.2",
    "@types/cors": "^2.8.12",
    "@types/express": "^4.17.13",
    "@types/joi": "^17.2.3",
    "@types/jsonwebtoken": "^8.5.8",
    "@types/lodash": "^4.14.182",
    "@types/module-alias": "^2.0.1",
    "@types/mongoose": "^5.11.97",
    "@types/nanoid": "^3.0.0",
    "@types/node": "^18.0.6",
    "@types/pino": "^7.0.5",
    "husky": "^8.0.1",
    "lint-staged": "^13.0.3",
    "nodemon": "^2.0.19",
    "prettier": "^2.7.1",
    "ts-node": "^10.9.1",
    "typescript": "^4.7.4"
  },
  "name": "nodejs_typescript",
  "version": "1.0.0",
  "main": "index.js",
  "license": "MIT",
  "lint-staged": {
    "*.{ts,tsx,js,css,md}": "prettier --write"
  },
  "_moduleAliases": {
    "@middleware": "dist/middleware",
    "@services": "dist/services"
  }
}
